<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
">
  
    <context:annotation-config/>
    
    <bean class="com.fpmislata.banco.persistencia.dao.impl.jdbc.ConnectionFactoryImplDataSource"/>
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.EntidadBancariaDAOImplHibernate"/>
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.CuentaDAOImplHibernate"/>
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.UsuarioDAOImplHibernate"/>
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.AdministradorDAOImplHibernate"/>
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.MovimientoDAOImplHibernate"/> 
    <bean class="com.fpmislata.banco.persistencia.dao.impl.hibernate.SucursalBancariaDAOImplHibernate"/>     
    <bean class="com.fpmislata.banco.persistencia.migration.DatabaseMigrationImplFlyway"/>
    <bean class="com.fpmislata.banco.common.json.impl.JSONConverterImplJackson"/>
    <bean class="com.fpmislata.banco.common.encrypting.PasswordEncryptingImplJasypt"/>
    <bean class="com.fpmislata.banco.servicio.Authentication"/>
     
</beans>
