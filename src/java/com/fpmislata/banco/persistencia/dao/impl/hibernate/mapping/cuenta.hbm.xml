<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.dominio.Cuenta" table="cuentas" >
        <id name="idCuenta" column="id" type="integer">
            <generator class="identity"/>
        </id>
        <property name="saldoCuenta" column="saldo" />
        <property name="idSucursal" column="idsucursal"/>
        <property name="tipoCuenta" column="tipo" />
        <property name="clienteCuenta" column="cliente"/>
    </class>
</hibernate-mapping>
